{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/sanctum/(.*)",
      "destination": "/sanctum/$1"
    },
    {
      "source": "/((?!api|sanctum).*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  }
}
